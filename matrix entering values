m = int(input("Enter the rows: "))
n = int(input("Enter the colums: "))
matrix = [] ; columns = []
for i in range(m):
    matrix += [0]
for j in range (n):
  columns += [0]
for i in range (m):
  matrix[i] = columns
for i in range (m):
  for j in range (n):
    print ('entry in row: ',i+1,' column: ',j+1)
    matrix[i][j] = int(input())
print (matrix)
